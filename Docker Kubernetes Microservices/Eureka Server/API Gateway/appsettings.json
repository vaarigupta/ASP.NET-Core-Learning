{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information",
      "Newtonsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "spring": {
    "application": { "name": "Api-Gateway" }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": false,
      "validateCertificates": false
    }
    //"server": {
    //  "enableSelfPreservation": false
    //}
  }
}
